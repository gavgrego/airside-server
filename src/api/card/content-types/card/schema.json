{
  "kind": "collectionType",
  "collectionName": "cards",
  "info": {
    "singularName": "card",
    "pluralName": "cards",
    "displayName": "Card",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "processor": {
      "type": "enumeration",
      "enum": [
        "Visa",
        "Amex",
        "Mastercard"
      ]
    },
    "bank": {
      "type": "enumeration",
      "enum": [
        "Chase",
        "Amex",
        "Barclays",
        "Citi",
        "Bank of America",
        "Capital One",
        "Wells Fargo"
      ]
    },
    "annualFee": {
      "type": "decimal"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "personalOrBiz": {
      "type": "enumeration",
      "enum": [
        "Personal",
        "Business"
      ]
    },
    "lounges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lounge.lounge",
      "mappedBy": "card"
    }
  }
